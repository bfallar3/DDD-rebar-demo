<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <configSections  xdt:Transform="InsertIfMissing">
    <section name="accenture.security.eso.service" type="System.Configuration.NameValueSectionHandler" xdt:Transform="InsertIfMissing" xdt:Locator="Match(name)"/>
  </configSections>

  <accenture.security.eso.service xdt:Transform="InsertIfMissing">
    <add key="Services:Issuer" value="{AD FS Issuer Name}" xdt:Transform="InsertIfMissing" xdt:Locator="Match(key)"/>
    <add key="Services:Thumbprint" value="{AD FS Issuer Thumbprint}" xdt:Transform="InsertIfMissing" xdt:Locator="Match(key)"/>
    <add key="Services:Identifier" value="{Service Identifier}" xdt:Transform="InsertIfMissing" xdt:Locator="Match(key)"/>
  </accenture.security.eso.service>
</configuration>