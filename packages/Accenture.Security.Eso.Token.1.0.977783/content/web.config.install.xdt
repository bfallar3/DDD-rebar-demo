<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <configSections xdt:Transform="InsertIfMissing">
    <section name="accenture.security.eso.token" type="System.Configuration.NameValueSectionHandler" xdt:Transform="InsertIfMissing" xdt:Locator="Match(name)"/>
  </configSections>

  <accenture.security.eso.token xdt:Transform="InsertIfMissing">
    <add key="Services:Endpoints:Jwt" value="http://localhost:{iisexpressport}/_sts/adfs" xdt:Transform="InsertIfMissing" xdt:Locator="Match(key)"/>
    <add key="Services:Identifier" value="{Service Identifier}" xdt:Transform="InsertIfMissing" xdt:Locator="Match(key)"/>
  </accenture.security.eso.token>
</configuration>